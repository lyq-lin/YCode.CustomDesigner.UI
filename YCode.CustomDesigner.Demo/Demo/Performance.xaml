<UserControl x:Class="YCode.CustomDesigner.Demo.Performance"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:YCode.CustomDesigner.Demo"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:ycode="clr-namespace:YCode.CustomDesigner.UI;assembly=YCode.CustomDesigner.UI"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">

    <UserControl.DataContext>
        <local:MainViewModel />
    </UserControl.DataContext>

    <Grid>
        <ycode:YCodeEditor Source="{Binding Source}" />
    </Grid>
</UserControl>
